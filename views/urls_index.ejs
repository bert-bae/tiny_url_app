<!DOCTYPE html>
<html lang="en">

  <% include ./partials/_head.ejs %>

  <body>
      <!-- Add your EJS code here -->

      <% include ./partials/_header.ejs %>

      <table>

        <% for (let link in urls) { %>
          <tr>
            <a href = <%= urls[link] %>><%= urls[link] %></a>
            <form method = "GET" action = "/urls/<%= link %>">
              <button>Edit</button>
            </form>

            <form method = "POST" action = "/urls/<%= link %>/delete">
              <button>Delete</button>
            </form>
          </tr><br>
        <% } %>

      </table>
      <h3><a href = "/urls/new">Shorten another link!</a></br></h3>

  </body>
</html>