<!DOCTYPE html>
<html lang="en">

  <% include ./partials/_head.ejs %>

  <body>
      <!-- Add your EJS code here -->

      <% include ./partials/_header.ejs %>


      <% if (user !== "") { %>
        <table>
          <% for (let link in urls) { %>
            <tr>
              <td><p><%= link %> - <a href = <%= urls[link] %>><%= urls[link] %></a></p></td>
              <td>
                <form method = "GET" action = "/urls/<%= link %>">
                  <button>Edit</button>
                </form>
              </td>
              <td>
                <form method = "POST" action = "/urls/<%= link %>/delete">
                  <button>Delete</button>
                </form>
              </td>
            </tr>
          <% } %>
        </table>
      <% } else { %>
        <p>Create an account to make your own short URL.</p>
      <% } %>

      <h3><a href = "/urls/new">Shorten another link!</a></br></h3>

  </body>
</html>







